<div class="container">
  <div class="row">
    <div class="login-wrapper-img one-half column">
      <div class="container">
        <div clas="row">
          <div class="">
            <img class="login-img-logo" src="/images/no-logo.png">
          </div>
        </div>
      </div>
    </div>
    <div class="login-wrapper one-half column">
      <div class="container">
        <div class="row">
          <h3>Welcome</h3>
          <h5>Please log in or register</h5>
          <form>
            <label for="usremail">Email:</label>
            <input type="email" id="usremail" class="login-txtbox" name="usremail" placeholder="example@host.com">
            <label for="usrpwd">Password:</label>
            <input type="password" id="usrpwd" class="login-txtbox" name="usrpwd" placeholder="Your password...">
            <div class="login-btn-wrap">
              <button type="submit">Log in</button>
            </div>
            <div class="login-btn-wrap">
              <a href="">Register</a>
            </div>
          </form>
        <div>
        <hr>
        <div class="row">
          <div class="login-btn-wrap">
            <button type="button">GOOGLE</button>
            <button type="button">FACEBOOK</button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>